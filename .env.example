# Server Configuration
PORT=3000
NODE_ENV=development
API_VERSION=v1
LOG_LEVEL=info

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/agentnexus
MONGODB_USER=
MONGODB_PASSWORD=
POSTGRES_URI=postgresql://postgres:password@localhost:5432/agentnexus
REDIS_URI=redis://localhost:6379

# Authentication
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRATION=1d
REFRESH_TOKEN_SECRET=your_refresh_token_secret
REFRESH_TOKEN_EXPIRATION=7d

# Blockchain Configuration
ENABLE_BLOCKCHAIN=true
POLYGON_RPC_URL=https://polygon-mumbai.infura.io/v3/your_infura_key
POLYGON_PRIVATE_KEY=your_private_key_here
REGISTRY_CONTRACT_ADDRESS=

# IPFS Configuration
IPFS_API_URL=https://ipfs.infura.io:5001
IPFS_PROJECT_ID=your_ipfs_project_id
IPFS_PROJECT_SECRET=your_ipfs_project_secret

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=
REDIS_CACHE_TTL=3600
ENABLE_CACHE=true

# External Services
OPENAI_API_KEY=your_openai_api_key
PINECONE_API_KEY=your_pinecone_api_key
PINECONE_ENVIRONMENT=your_pinecone_environment

# Security
CORS_ORIGIN=*
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100

# Logging
LOG_FILE_PATH=logs/app.log
LOG_MAX_SIZE=10m
LOG_MAX_FILES=7

# Validation Pipeline
VALIDATION_WORKERS=3
VALIDATION_TIMEOUT=300000

# Sandbox Environment
SANDBOX_CONTAINER_LIMIT=10
SANDBOX_MEMORY_LIMIT=4G
SANDBOX_CPU_LIMIT=2

# Feature Flags
ENABLE_AI_SEARCH=true
ENABLE_VALIDATION=true
ENABLE_SANDBOX=true

# Advanced Features
QUANTUM_RESISTANT_IDENTITY=true
NEURAL_FINGERPRINTING=true
FEDERATED_VALIDATION=true
EMERGENT_CAPABILITY_DISCOVERY=true

# Agent Configuration
MAX_AGENTS_PER_USER=10
DEFAULT_AGENT_TIMEOUT=30000 